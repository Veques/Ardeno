<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!-- BASE -->
    <Color x:Key="LeadingColor">#FC4C00</Color>
    <Color x:Key="Font">WhiteSmoke</Color>
    
    <!-- LOGIN -->
    <SolidColorBrush x:Key="FontColor" Color="{DynamicResource Font}"/>
    <SolidColorBrush x:Key="ThemeColor" Color="{DynamicResource LeadingColor}"/>


</ResourceDictionary>